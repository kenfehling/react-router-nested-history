{
  "parser": "babel-eslint",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors"
  ],
  "rules": {
    "prefer-arrow-callback": 2,
    "semi": [ 2, "never" ],
    "no-extra-semi": 2,
    "no-unused-vars": 1
  }
}