{
  "name": "react-router-nested-history",
  "version": "4.0.0-alpha.6",
  "description": "A library to help build React apps with nested tabs and other containers that have their own history, complete with automatic altering of the browser's back and forward history to make it behave as you would expect",
  "author": "Ken Fehling",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src tests",
    "test": "jest",
    "test:watch": "jest --watch",
    "flow": "flow",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-router-nested-history.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/react-router-nested-history.min.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:examples": "cd examples/react; npm run build",
    "publish:examples": "git subtree push --prefix examples/react/build origin gh-pages"
  },
  "keywords": [
    "history"
  ],
  "dependencies": {
    "date-fns": "^1.21.1",
    "history": "^4.4.0",
    "invariant": "^2.2.2",
    "lodash": "^4.2.0",
    "path-to-regexp": "^1.7.0",
    "promise-queue": "^2.2.3",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0-beta2",
    "redux-thunk": "^2.1.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.3.13",
    "enzyme": "^2.6.0",
    "eslint": "^3.7.1",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.36.0",
    "jest-cli": "^16.0.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "4.0.0-alpha.6",
    "react-with-context": "^1.0.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.11.0"
  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "4.0.0-alpha.6"
  }
}
