{
  "name": "react-router-nested-history",
  "version": "4.0.0-beta.6",
  "description": "A library to help build React apps with nested tabs and other containers that have their own history, complete with automatic altering of the browser's back and forward history to make it behave as you would expect",
  "author": "Ken Fehling",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenfehling/react-router-nested-history"
  },
  "bugs": "https://github.com/kenfehling/react-router-nested-history/issues",
  "main": "dist/react-router-nested-history.min.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src tests",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e-setup": "cd test/e2e; npm run e2e-setup",
    "test:e2e-server": "cd test/e2e; npm start",
    "test:e2e": "cd test/e2e; npm test",
    "build:umd": "webpack src/index.js dist/react-router-nested-history.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/react-router-nested-history.min.js --config webpack.config.production.js",
    "build": "npm run build:umd && npm run build:umd:min",
    "build:examples": "cd examples/react; npm uninstall react-router-nested-history; npm install react-router-nested-history; npm run build",
    "publish:examples": "git subtree push --prefix examples/react origin heroku"
  },
  "keywords": [
    "nested",
    "history",
    "react",
    "router",
    "tree"
  ],
  "dependencies": {
    "history": "^4.5.1",
    "invariant": "^2.2.2",
    "path-to-regexp": "^1.7.0",
    "promise-queue": "^2.2.3",
    "ramda": "^0.23.0",
    "react-motion": "^0.4.7",
    "react-redux": "^4.4.6",
    "react-router-transition": "0.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "store": "^1.3.20",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@types/history": "^4.5.0",
    "@types/ramda": "0.0.2",
    "@types/react": "^15.0.11",
    "@types/react-dom": "^0.14.20",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.45",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.3.13",
    "eslint": "^3.7.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "4.0.0-beta.6",
    "react-router-dom": "4.0.0-beta.6",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.6",
    "ts-jest": "^18.0.1",
    "typescript": "^2.1.5",
    "webpack": "^2.2.0"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15",
    "react-router": "4.0.0-beta.6",
    "react-router-dom": "4.0.0-beta.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/(unit|integration)/.*test.*\\.(ts|tsx|js|jsx)$"
  }
}
